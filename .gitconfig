[user]
  name = shimada
  # email = xxxxxx@gmail.com

[color]
  ui = auto

[alias]
  amend = commit --amend
  br = branch
  co = checkout
  di = diff
  dic = diff --cached
  lo = log --graph --date=short --decorate=short --pretty=format:'%Cgreen%h %Creset%cd %Cblue%cn %Cred%d %Creset%s'
  st = status -b
  sw = switch
  unci = reset --soft 'HEAD^'

[core]
  editor = vim
[commit]
	template = ~/.git_commit_template
